-- -- -- -- |	// [function] get_open_physical_page | void TODO: this is not void
-- -- -- -- |	//=========================================================================
-- -- -- -- |	@@get_open_physical_page
-- -- -- -- |	//returns physical page number that is available for incoming page
-- -- -- -- |	//may or may not result in page eviction
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	//calculate address where return value should be stored
00 00 07 D2 |	    03 ALUM //subtract
00 00 07 D4 |	    WBASE ALUA //original FP
00 00 07 D6 |	    54 ALUB // -84
00 00 07 D8 |	    ALUR GPH //GPH = result address = FP - 84
-- -- -- -- |	
-- -- -- -- |	//look for open pages, which we can use without evicting anything
-- -- -- -- |	//open pages have a process ID of 0
-- -- -- -- |	
-- -- -- -- |	//iterate over physical page map
-- -- -- -- |	    //point RMEM to physical page map array
00 00 07 DA |	        IADF RBASE
00 00 07 DC |	        SKIP PC
00 00 07 DE |	        ::physical_page_map
00 00 07 E2 |	        00 ROFST
-- -- -- -- |	
00 00 07 E4 |	    00 GPA //GPA = index
00 00 07 E6 |	    10 GPB //GPB = max_index
-- -- -- -- |	
-- -- -- -- |	    @open_page_loop
-- -- -- -- |	    //check if we're at the end of the loop (index == 16?)
00 00 07 E8 |	        GPA COMPA
00 00 07 EA |	        GPB COMPB
00 00 07 EC |	        COMPR PC
00 00 07 EE |	        :open_page_loop_end
00 00 07 F2 |	        :open_page_loop_go
-- -- -- -- |	
-- -- -- -- |	    @open_page_loop_go
-- -- -- -- |	    //calculate offset from index
00 00 07 F6 |	        02 ALUM //multiply mode
00 00 07 F8 |	        GPA ALUA // ALUA = index
00 00 07 FA |	        14 ALUB //20 bytes per entry
00 00 07 FC |	        ALUR ROFST
-- -- -- -- |	
-- -- -- -- |	    //read process ID (offset 0)
00 00 07 FE |	    RMEM GPC //GPC = process id
-- -- -- -- |	
-- -- -- -- |	    //process id == 0?
00 00 08 00 |	        GPC COMPA
00 00 08 02 |	        00 COMPB
00 00 08 04 |	        COMPR PC
00 00 08 06 |	        :open_page_proc_id_0
00 00 08 0A |	        :open_page_loop_next
-- -- -- -- |	    
-- -- -- -- |	    @open_page_proc_id_0
-- -- -- -- |	    // found an open page
-- -- -- -- |	    // return the index as the open physical page
00 00 08 0E |	        WBASE ALUA //store FP somewhere
00 00 08 10 |	        GPH WBASE //point WMEM to the result address
00 00 08 12 |	        00 WOFST
00 00 08 14 |	        GPA WMEM //result = index into physical page map
00 00 08 16 |	        ALUA WBASE //restore FP
00 00 08 18 |	        LINK PC //return
-- -- -- -- |	
-- -- -- -- |	    
-- -- -- -- |	
-- -- -- -- |	    @open_page_loop_next
-- -- -- -- |	    //increment index
00 00 08 1A |	        01 ALUM //add mode
00 00 08 1C |	        GPA ALUA
00 00 08 1E |	        01 ALUB
00 00 08 20 |	        ALUR GPA //index += 1
-- -- -- -- |	    //go to start of loop
00 00 08 22 |	    IADN PC
00 00 08 24 |	    :open_page_loop
-- -- -- -- |	
-- -- -- -- |	    //TODO implement this
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	@open_page_loop_end
-- -- -- -- |	//no open pages
00 00 08 28 |	BREAK
