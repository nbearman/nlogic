-- -- -- -- |	//TODO implement this
00 00 08 2A |	00 RBASE
00 00 08 2C |	00 ROFST
00 00 08 2E |	7F RMEM
-- -- -- -- |	
-- -- -- -- |	//return
00 00 08 30 |	LINK PC
