00 00 05 04 |	IADF WBASE
00 00 05 06 |	SKIP PC
00 00 05 08 |	::KERNEL_STACK
00 00 05 0C |	00 WOFST
00 00 05 0E |	GPA WMEM
00 00 05 10 |	04 WOFST
00 00 05 12 |	GPB WMEM
00 00 05 14 |	08 WOFST
00 00 05 16 |	GPC WMEM
00 00 05 18 |	0C WOFST
00 00 05 1A |	GPD WMEM
00 00 05 1C |	10 WOFST
00 00 05 1E |	GPE WMEM
00 00 05 20 |	14 WOFST
00 00 05 22 |	GPF WMEM
00 00 05 24 |	18 WOFST
00 00 05 26 |	GPG WMEM
00 00 05 28 |	1C WOFST
00 00 05 2A |	GPH WMEM
-- -- -- -- |	
00 00 05 2C |	20 WOFST
00 00 05 2E |	COMPA WMEM
00 00 05 30 |	24 WOFST
00 00 05 32 |	COMPB WMEM
00 00 05 34 |	28 WOFST
00 00 05 36 |	RBASE WMEM
00 00 05 38 |	2C WOFST
00 00 05 3A |	ROFST WMEM
00 00 05 3C |	30 WOFST
00 00 05 3E |	ALUM WMEM
00 00 05 40 |	34 WOFST
00 00 05 42 |	ALUA WMEM
00 00 05 44 |	38 WOFST
00 00 05 46 |	ALUB WMEM
00 00 05 48 |	3C WOFST
00 00 05 4A |	FPUM WMEM
00 00 05 4C |	40 WOFST
00 00 05 4E |	FPUA WMEM
00 00 05 50 |	44 WOFST
00 00 05 52 |	FPUB WMEM
00 00 05 54 |	48 WOFST
00 00 05 56 |	DMEM00 WMEM
00 00 05 58 |	4C WOFST
00 00 05 5A |	DMEM04 WMEM
00 00 05 5C |	50 WOFST
-- -- -- -- |	
-- -- -- -- |	FRAME_START
-- -- -- -- |	//=========================
-- -- -- -- |	// interrupt handler stack layout / local variables
-- -- -- -- |	//==========
-- -- -- -- |	// 0x00 |   uint    |   FLAG
-- -- -- -- |	// 0x04 |   uint    |   faulted PTE
-- -- -- -- |	// 0x08 |   uint    |   faulted virtual address
-- -- -- -- |	// 0x0C |   uint    |   faulted PTE disk block
-- -- -- -- |	// 0x10 |   uint    |   virtual page number from faulted virtual address
-- -- -- -- |	// 0x14 |   uint    |   active process ID
-- -- -- -- |	// 0x18 |   uint    |   active process page directory physical page
-- -- -- -- |	// 0x1C |   uint    |   physical page new page is moved into
-- -- -- -- |	//=========================
-- -- -- -- |	
-- -- -- -- |	    STACK flag_val 04
-- -- -- -- |	    STACK faulted_pte 04
-- -- -- -- |	    STACK faulted_va 04
-- -- -- -- |	    STACK faulted_pte_block 04
-- -- -- -- |	    STACK faulted_va_vpage_num 04
-- -- -- -- |	    STACK active_process_id 04
-- -- -- -- |	    STACK active_process_page_directory_ppage 04
-- -- -- -- |	    STACK target_ppage 04
-- -- -- -- |	    STACK fetched_pde 04 // possibly different than faulted PTE if the leaf page faulted
-- -- -- -- |	    STACK fetched_pte 04 // retrieved by following the PDE if leaf page faulted
-- -- -- -- |	                            //should be the same as faulted PTE in that case
-- -- -- -- |	    STACK fetched_pde_kpa 04
-- -- -- -- |	    STACK fetched_pte_kpa 04
-- -- -- -- |	
-- -- -- -- |	//add stack frame; registers are dumped to bottom of the stack, so we don't need to save a frame pointer
-- -- -- -- |	//  (specifically because handler runs at the very bottom of the kernel's stack)
-- -- -- -- |	//(just set WBASE to 00 when it's time to retrieve them)
00 00 05 5E |	01 ALUM //add
00 00 05 60 |	WBASE ALUA
00 00 05 62 |	WOFST ALUB
00 00 05 64 |	ALUR WBASE //WBASE = FP == start of first frame on the stack
-- -- -- -- |	    //before this is the dumped contents of the processor before the interrupt
00 00 05 66 |	ISIZE_FRAME WOFST //WOFST = SP == frame size
-- -- -- -- |	
-- -- -- -- |	//store FLAG in local variable
00 00 05 68 |	    WBASE RBASE
00 00 05 6A |	    ISTACK_flag_val ROFST
00 00 05 6C |	    FLAG RMEM
-- -- -- -- |	
-- -- -- -- |	//determine the cause of the interrupt
-- -- -- -- |	    //mask flag to check if interrupt was raised on MMU channel
00 00 05 6E |	    FLAG ALUA
00 00 05 70 |	    IADF ALUB
00 00 05 72 |	    SKIP PC
00 00 05 74 |	    00 00 00 01 //mask for the first channel (MMU)
00 00 05 78 |	    08 ALUM //AND mode
00 00 05 7A |	    ALUR COMPA
00 00 05 7C |	    00 COMPB //if flag channel is 1, jump to mmu interrupt handler
00 00 05 7E |	    COMPR PC
00 00 05 80 |	    :non_mmu_interrupt
00 00 05 84 |	    :mmu_interrupt
-- -- -- -- |	
-- -- -- -- |	@non_mmu_interrupt
-- -- -- -- |	//if interrupt is not from MMU
-- -- -- -- |	    //do nothing if the interrupt is from anywhere besides the MMU
00 00 05 88 |	    7F FLAG
-- -- -- -- |	
-- -- -- -- |	@mmu_interrupt
-- -- -- -- |	//else interrupt is from MMU
-- -- -- -- |	//retrieve the faulted PTE from the MMU
00 00 05 8A |	    IADF RBASE
00 00 05 8C |	    SKIP PC
00 00 05 8E |	    00 00 10 00 //MMU VA
00 00 05 92 |	    0C ROFST //0x0C == faulted PTE register offset in MMU address range
00 00 05 94 |	    RMEM GPA
-- -- -- -- |	
-- -- -- -- |	//retrieve the faulted address from the MMU
00 00 05 96 |	    10 ROFST //0x10 == faulted VA register offset in MMU address range
00 00 05 98 |	    RMEM GPB
-- -- -- -- |	
-- -- -- -- |	//store faulted PTE and faulted virtual address in local variables
00 00 05 9A |	    WBASE RBASE
00 00 05 9C |	    ISTACK_faulted_pte ROFST
00 00 05 9E |	    GPA RMEM
00 00 05 A0 |	    ISTACK_faulted_va ROFST
00 00 05 A2 |	    GPB RMEM
-- -- -- -- |	
-- -- -- -- |	//get the active process ID and page directory physical page from kernel memory
00 00 05 A4 |	    00 ROFST
00 00 05 A6 |	    IADF RBASE
00 00 05 A8 |	    SKIP PC
00 00 05 AA |	    ::ACTIVE_PROCESS_ID
00 00 05 AE |	    RMEM GPC
00 00 05 B0 |	    IADF RBASE
00 00 05 B2 |	    SKIP PC
00 00 05 B4 |	    ::ACTIVE_PROCESS_PAGE_DIRECTORY_PHYSICAL_PAGE
00 00 05 B8 |	    RMEM GPD
-- -- -- -- |	
-- -- -- -- |	//store active process ID and page directory physical page in local variables
00 00 05 BA |	    WBASE RBASE
00 00 05 BC |	    ISTACK_active_process_id ROFST
00 00 05 BE |	    GPC RMEM
00 00 05 C0 |	    ISTACK_active_process_page_directory_ppage ROFST
00 00 05 C2 |	    GPD RMEM
-- -- -- -- |	
-- -- -- -- |	//check PTE protections
-- -- -- -- |	//get them from PTE
00 00 05 C4 |	    GPA ALUA //PTE to ALU
00 00 05 C6 |	    IADF ALUB //mask to ALU
00 00 05 C8 |	    SKIP PC
00 00 05 CA |	    C0 00 00 00 //mask for the RW bits of the PTE
00 00 05 CE |	    08 ALUM //AND mode
-- -- -- -- |	
00 00 05 D0 |	    ALUR ALUA //RW bits to ALU
00 00 05 D2 |	    1E ALUB //shift 30 bits right
00 00 05 D4 |	    06 ALUM //right shift mode
-- -- -- -- |	
-- -- -- -- |	//see which of 00, 01, 10, 11 the RW bits are
00 00 05 D6 |	ALUR COMPA
00 00 05 D8 |	00 COMPB //0b00
00 00 05 DA |	COMPR PC
00 00 05 DC |	:r0w0
00 00 05 E0 |	:_r0w0
-- -- -- -- |	@_r0w0
00 00 05 E4 |	01 COMPB //0b01
00 00 05 E6 |	COMPR PC
00 00 05 E8 |	:r0w1
00 00 05 EC |	:_r0w1
-- -- -- -- |	@_r0w1
00 00 05 F0 |	02 COMPB //0b10
00 00 05 F2 |	COMPR PC
00 00 05 F4 |	:r1w0
00 00 05 F8 |	:_r1w0
-- -- -- -- |	@_r1w0
00 00 05 FC |	7F FLAG //halt; RW was 11, no page fault should have occurred
-- -- -- -- |	
-- -- -- -- |	@r0w0
-- -- -- -- |	//not mapped (possibly syscall)
00 00 05 FE |	7F FLAG //TODO for now, just halt
-- -- -- -- |	
-- -- -- -- |	@r1w0
-- -- -- -- |	//page is readable but not writable
-- -- -- -- |	//either a shared page or a clean page
-- -- -- -- |	//page needs to be split or marked as dirty
00 00 06 00 |	7F FLAG //TODO for now, just halt
-- -- -- -- |	
-- -- -- -- |	@r0w1
-- -- -- -- |	//not readable and "writable" indicates the page is mapped but paged out
-- -- -- -- |	//page not resident
-- -- -- -- |	
-- -- -- -- |	//push function address onto stack
00 00 06 02 |	IADF WMEM
00 00 06 04 |	SKIP PC
00 00 06 06 |	::get_open_physical_page
-- -- -- -- |	
00 00 06 0A |	01 ALUM //add mode
00 00 06 0C |	ISIZE_FRAME ALUA //stack frame size
00 00 06 0E |	04 ALUB //add 4 for room for the target address, which we just pushed to the stack
-- -- -- -- |	//TODO theoretically this is known at compile time; add macro for ISIZE_FRAME+X ?
00 00 06 10 |	ALUR WOFST //SP = size of stack frame + 4
-- -- -- -- |	
-- -- -- -- |	//jump to function
00 00 06 12 |	RTRN LINK
00 00 06 14 |	IADN PC
00 00 06 16 |	::FUNC
-- -- -- -- |	
-- -- -- -- |	//result of function call is target physical page number
-- -- -- -- |	//pop result from stack
00 00 06 1A |	    ISIZE_FRAME WOFST //size of stack frame, top of stack
00 00 06 1C |	    WMEM GPH
-- -- -- -- |	//store in local variable
00 00 06 1E |	    WBASE RBASE
00 00 06 20 |	    ISTACK_target_ppage ROFST
00 00 06 22 |	    GPH RMEM
-- -- -- -- |	
-- -- -- -- |	//load page from disk
-- -- -- -- |	//get disk block from PTE
00 00 06 24 |	    GPA ALUA //PTE to ALU
00 00 06 26 |	    IADF ALUB //mask to ALU
00 00 06 28 |	    SKIP PC
00 00 06 2A |	    00 0F FF FF //disk block mask
00 00 06 2E |	    08 ALUM //AND mode
00 00 06 30 |	    ALUR GPE
-- -- -- -- |	//get virtual page / virtual directory number from virtual address
00 00 06 32 |	    GPB ALUA //virtual address to ALU
00 00 06 34 |	    IADF ALUB //mask to ALU
00 00 06 36 |	    SKIP PC
00 00 06 38 |	    FF FF F0 00 //remove physical offset part of virtual address
00 00 06 3C |	    ALUR GPF
-- -- -- -- |	
-- -- -- -- |	//store disk block and virtual page number in local variables
00 00 06 3E |	    WBASE RBASE
00 00 06 40 |	    ISTACK_faulted_pte_block ROFST
00 00 06 42 |	    GPE RMEM
00 00 06 44 |	    ISTACK_faulted_va_vpage_num ROFST
00 00 06 46 |	    GPF RMEM
-- -- -- -- |	
-- -- -- -- |	//load from disk
-- -- -- -- |	    //point RMEM to virtual disk
00 00 06 48 |	        IADF RBASE
00 00 06 4A |	        SKIP PC
00 00 06 4C |	        00 00 10 1C //MMIO starts at VA 1000
-- -- -- -- |	    //tell disk the target physical page (stored in GPH)
00 00 06 50 |	        00 ROFST
00 00 06 52 |	        GPH RMEM
-- -- -- -- |	    //tell disk target disk block (stored in GPE)
00 00 06 54 |	        04 ROFST
00 00 06 56 |	        GPE RMEM
-- -- -- -- |	    //use read mode
00 00 06 58 |	        08 ROFST
00 00 06 5A |	        00 RMEM
-- -- -- -- |	    //initiate transfer from disk to memory
00 00 06 5C |	        0C ROFST
00 00 06 5E |	        01 RMEM
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	//update physical page map
-- -- -- -- |	    //point RMEM to physical page map array
00 00 06 60 |	        IADF RBASE
00 00 06 62 |	        SKIP PC
00 00 06 64 |	        ::physical_page_map
-- -- -- -- |	
-- -- -- -- |	    //calculate offset of target physical page entry
00 00 06 68 |	        02 ALUM //multiply
00 00 06 6A |	        GPH ALUA //target physical page
00 00 06 6C |	        14 ALUB //20 bytes per entry
00 00 06 6E |	        ALUR ROFST //physical page map offset
-- -- -- -- |	
00 00 06 70 |	    GPC RMEM //set process ID to user process
-- -- -- -- |	
-- -- -- -- |	    //move to next field in entry
00 00 06 72 |	        01 ALUM //add
00 00 06 74 |	        ROFST ALUA
00 00 06 76 |	        04 ALUB
00 00 06 78 |	        ALUR ROFST
-- -- -- -- |	
00 00 06 7A |	    GPD RMEM //set directory physical page to user directory
-- -- -- -- |	
-- -- -- -- |	    //move to next field
00 00 06 7C |	        ALUR ALUA
00 00 06 7E |	        ALUR ROFST
-- -- -- -- |	        
00 00 06 80 |	    GPF RMEM //set virtual page/directory number to that of the faulted address
-- -- -- -- |	
-- -- -- -- |	    //move to next field
00 00 06 82 |	        ALUR ALUA
00 00 06 84 |	        ALUR ROFST
-- -- -- -- |	
00 00 06 86 |	    01 RMEM //only one process references this page
-- -- -- -- |	
-- -- -- -- |	    //move to next field
00 00 06 88 |	        ALUR ALUA
00 00 06 8A |	        ALUR ROFST
-- -- -- -- |	
00 00 06 8C |	    GPE RMEM //store origin disk block as the disk block
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	//update process map
-- -- -- -- |	    //point RMEM to process map
00 00 06 8E |	        IADF RBASE
00 00 06 90 |	        SKIP PC
00 00 06 92 |	        ::process_map
-- -- -- -- |	
-- -- -- -- |	    //calculate offset of target process entry ((process ID - 1) is index into map)
-- -- -- -- |	        // (16 * (PID - 1)) + 8
00 00 06 96 |	        03 ALUM //subtract
00 00 06 98 |	        GPC ALUA //process ID
00 00 06 9A |	        01 ALUB //minus 1
00 00 06 9C |	        ALUR ALUA //ALUA = PID - 1
-- -- -- -- |	
00 00 06 9E |	        10 ALUB //16 byte per entry
00 00 06 A0 |	        02 ALUM //multiply
00 00 06 A2 |	        ALUR ALUA //process entry map offset
00 00 06 A4 |	        08 ALUB //offset into entry for number of resident pages
00 00 06 A6 |	        01 ALUM //add
00 00 06 A8 |	        ALUR ROFST //total offset into process map
-- -- -- -- |	
-- -- -- -- |	    //increment number of resident pages for this process
00 00 06 AA |	        RMEM ALUA
00 00 06 AC |	        01 ALUB
00 00 06 AE |	        ALUR RMEM
-- -- -- -- |	
-- -- -- -- |	//update page table
-- -- -- -- |	//TODO how do we know if we loaded a page table or a leaf page?
-- -- -- -- |	//  if we loaded a leaf page, we need to update the page table
-- -- -- -- |	//  but if we loaded a page table, we need to update the page directory
-- -- -- -- |	//      Use the page table to see which of the PDE/PTE was protected -> caused this fault
-- -- -- -- |	//  traverse the page table: the first PDE/PTE with matching protection bits
-- -- -- -- |	//  was the one that failed
-- -- -- -- |	//      this is true because !R,W always means the page is mapped but not resident.
-- -- -- -- |	//      if we are trying to load a leaf page, it is not possible that the parent table
-- -- -- -- |	//      is !R,W (evicted), because we would have faulted on accessing that table before
-- -- -- -- |	//      we ever knew we needed to bring this leaf page into memory.
-- -- -- -- |	//      therefore, if the PDE is !R,W, we are definitely loading a page table
-- -- -- -- |	//      ...if PDE caused fault: we are loading a page table into memory
-- -- -- -- |	//      ...if PTE caused fault: we are loading a leaf page into memory
-- -- -- -- |	
-- -- -- -- |	//update page table
-- -- -- -- |	// get the PDE (don't just use the faulted PTE; we still don't know if that is a PDE or PTE)
-- -- -- -- |	//  if the PDE was the one that faulted, it should have been !R,W
-- -- -- -- |	//      now the page table it points to is in memory, mark the PDE as R,!W
-- -- -- -- |	//          the page table is write protected because it is clean compared to disk
-- -- -- -- |	//      mark the PDE as !F,!D since the page table is clean and newly loaded
-- -- -- -- |	//          handling this fault does not count as accessing/referencing the newly loaded page table
-- -- -- -- |	//          a subsequent read, or handling a PTE fault, would mark the PDE as referenced, though
-- -- -- -- |	//      update the PDE with the new physical page where the page table was put (local var at 0x1C)
-- -- -- -- |	//      done
-- -- -- -- |	//  if the PDE was not the one that faulted, it should have been R,W
-- -- -- -- |	//      mark the PDE as F,D since we will be updating the page table with the newly loaded leaf page's information
-- -- -- -- |	//          F bit should have been set before MMU faulted on the page anyway, since we read the page table
-- -- -- -- |	//      the PTE should have been the one that faulted; the leaf page was just loaded into memory
-- -- -- -- |	//          the PTE should be !R,W
-- -- -- -- |	//          get the PTE by following the PDE to the page table
-- -- -- -- |	//          set the PTE to R,!W
-- -- -- -- |	//              R because the page is now resident in memory and safe to read
-- -- -- -- |	//              !W because the page is clean compared to disk (it was just loaded)
-- -- -- -- |	//          mark the PTE as !F,!D since the leaf page is clean and newly loaded
-- -- -- -- |	//              F bit will be set when the instruction is retried, but loading the page right now doesn't count as reference
-- -- -- -- |	//          update the PTE with the new physical page where the page table was put (local var at 0x1C)
-- -- -- -- |	//      done
-- -- -- -- |	
-- -- -- -- |	//get PDE
00 00 06 B0 |	    05 ALUM //left shift
00 00 06 B2 |	    GPD ALUA //active process page directory physical page
00 00 06 B4 |	    0C ALUB //12 bits
00 00 06 B6 |	    ALUR GPA //GPA = page directory base address
-- -- -- -- |	
-- -- -- -- |	    //page table number = (virtual address & 0xFFC00000) >> 22
00 00 06 B8 |	    08 ALUM //AND
00 00 06 BA |	    GPB ALUA //virtual address
00 00 06 BC |	    IADF ALUB //mask for first 12 bits
00 00 06 BE |	    SKIP PC
00 00 06 C0 |	    FF C0 00 00
-- -- -- -- |	
00 00 06 C4 |	    ALUR ALUA
00 00 06 C6 |	    06 ALUM //right shift
00 00 06 C8 |	    14 ALUB //20 bits (page number is given by shifting 22 bits, but offset into page table is page number * 4, so shift left 2 bits)
-- -- -- -- |	
00 00 06 CA |	    ALUR ALUB
00 00 06 CC |	    GPA ALUA
00 00 06 CE |	    07 ALUM //OR
-- -- -- -- |	        //TODO setting OR then AND? probably do the same thing in this case
-- -- -- -- |	        // probably need to swap the next two instructions: use OR, retrieve result, then set AND
00 00 06 D0 |	    01 ALUM //add
00 00 06 D2 |	    ALUR ALUA //page directory entry physical address
00 00 06 D4 |	    IADF ALUB //add 0x00 3F 00 00 to get kernel virtual address of physical address
00 00 06 D6 |	    SKIP PC
00 00 06 D8 |	    00 3F 00 00
-- -- -- -- |	
-- -- -- -- |	    //save kernel's VA of the PDE in local variable; we'll need to write the updated PDE back later
00 00 06 DC |	    WBASE RBASE
00 00 06 DE |	    ISTACK_fetched_pde_kpa ROFST
00 00 06 E0 |	    ALUR RMEM
-- -- -- -- |	
-- -- -- -- |	    //set RBASE to point to the PDE for the faulted address (may or may not be the PDE/PTE that faulted)
00 00 06 E2 |	    ALUR RBASE
00 00 06 E4 |	    00 ROFST
-- -- -- -- |	
-- -- -- -- |	//check if PDE or PTE faulted
-- -- -- -- |	//  PDE faulted if the PDE is !R,W
-- -- -- -- |	//  else PTE faulted (if PTE faulted, PDE is necessarily R,W, else it would have faulted first)
-- -- -- -- |	    //extract RW bits from PDE
00 00 06 E6 |	        RMEM ALUA //PDE to ALU
-- -- -- -- |	
-- -- -- -- |	            //store PDE in local variable for later
00 00 06 E8 |	            WBASE RBASE
00 00 06 EA |	            ISTACK_fetched_pde ROFST
00 00 06 EC |	            ALUA RMEM
-- -- -- -- |	
00 00 06 EE |	        IADF ALUB //protection mask to ALU
00 00 06 F0 |	        SKIP PC
00 00 06 F2 |	        C0 00 00 00 //mask for the RW bits of the PDE
00 00 06 F6 |	        08 ALUM //AND mode
-- -- -- -- |	
00 00 06 F8 |	        ALUR ALUA //RW bits to ALU
00 00 06 FA |	        1E ALUB //shift 30 bits right
00 00 06 FC |	        06 ALUM //right shift mode
-- -- -- -- |	
-- -- -- -- |	    //compare PDE RW bits to !R,W (0x01)
00 00 06 FE |	        ALUR COMPA //RW bits to comparator
00 00 07 00 |	        01 COMPB //!R,W (0x01) to comparator
00 00 07 02 |	        COMPR PC
00 00 07 04 |	        :table_caused_r0w1 //the PDE faulted, so we just loaded a page table into memory
00 00 07 08 |	        :leaf_caused_r0w1 //the PTE faulted, so we just loaded a leaf page into memory
-- -- -- -- |	
-- -- -- -- |	@table_caused_r0w1 //IF CLAUSE (PDE faulted)
-- -- -- -- |	//if PDE faulted, we just loaded a page table in; update the PDE
-- -- -- -- |	//  update the PDE to be R,!W,!F,!D
00 00 07 0C |	    7F FLAG // TODO implement this
-- -- -- -- |	
-- -- -- -- |	// skip else clause
00 00 07 0E |	IADN PC
00 00 07 10 |	:conclude_r0w1
-- -- -- -- |	
-- -- -- -- |	@leaf_caused_r0w1 //ELSE CLAUSE (PTE faulted)
-- -- -- -- |	//else if PTE faulted, we just loaded a leaf page in; update the PDE and PTE
-- -- -- -- |	    //fetch the PTE through the PDE
-- -- -- -- |	        //we know the page table is resident, since the PDE didn't fault
-- -- -- -- |	        //therefore PDE holds the physical page of the PTE
-- -- -- -- |	
-- -- -- -- |	        //page table base address == (PDE & 0x000FFFFF) << 12
00 00 07 14 |	        WBASE RBASE
00 00 07 16 |	        ISTACK_fetched_pde ROFST
00 00 07 18 |	        RMEM ALUA //PDE to ALU
00 00 07 1A |	        IADF ALUB //mask to ALU
00 00 07 1C |	        SKIP PC
00 00 07 1E |	        00 0F FF FF //physical page mask
00 00 07 22 |	        08 ALUM //AND mode
-- -- -- -- |	
00 00 07 24 |	        ALUR ALUA //physical page number of page table
00 00 07 26 |	        0C ALUB //12 bits
00 00 07 28 |	        05 ALUM //left shift
00 00 07 2A |	        ALUR GPA //GPA = page table base address
-- -- -- -- |	
-- -- -- -- |	        //page number = (virtual address & 0x003FF000) >> 12
-- -- -- -- |	            // PTEs are 4 bytes, so address is (table addr + (page number * 4))
-- -- -- -- |	            // skip the *4 by shifting only 10 bits instead of 12
-- -- -- -- |	        //(virtual address & 0x003FF000) >> 10
00 00 07 2C |	        ISTACK_faulted_va ROFST
00 00 07 2E |	        RMEM ALUA //VA to ALU
00 00 07 30 |	        IADF ALUB //mask to ALU
00 00 07 32 |	        SKIP PC
00 00 07 34 |	        00 3F F0 00 //page number mask
00 00 07 38 |	        08 ALUM //AND
-- -- -- -- |	        
00 00 07 3A |	        ALUR ALUA // page number of PTE (from PDE, unshifted)
00 00 07 3C |	        06 ALUM //right shift
00 00 07 3E |	        0A ALUB //10 bits
-- -- -- -- |	
00 00 07 40 |	        ALUR ALUB //page number * 4
00 00 07 42 |	        GPA ALUA //page table base address
00 00 07 44 |	        07 ALUM //OR
-- -- -- -- |	
00 00 07 46 |	        ALUR ALUA //page table entry physical address to ALU
00 00 07 48 |	        IADF ALUB //add 0x00 3F 00 00 to get kernel virtual address of any physical address
00 00 07 4A |	        SKIP PC
00 00 07 4C |	        00 3F 00 00 //PA->kernel VA offset
00 00 07 50 |	        01 ALUM //ADD
00 00 07 52 |	        ALUR RBASE //PTE address in kernel VA
00 00 07 54 |	        00 ROFST //RMEM points to PTE
-- -- -- -- |	
00 00 07 56 |	        RMEM GPB //save the PTE somewhere
00 00 07 58 |	        WBASE RBASE
00 00 07 5A |	        ISTACK_fetched_pte ROFST
00 00 07 5C |	        GPB RMEM //store the fetched PTE in local variable
-- -- -- -- |	
00 00 07 5E |	        ISTACK_fetched_pte_kpa ROFST
00 00 07 60 |	        ALUR RMEM //store kernel's VA of PTE in local variable; we'll need to write the updated PTE back
-- -- -- -- |	
-- -- -- -- |	    //assert that the PTE is !R,W
-- -- -- -- |	        //get the protection bits
00 00 07 62 |	            GPB ALUA //PTE to ALU
00 00 07 64 |	            IADF ALUB //mask to ALU
00 00 07 66 |	            SKIP PC
00 00 07 68 |	            C0 00 00 00 //mask for the RW bits of the PTE
00 00 07 6C |	            08 ALUM //AND
-- -- -- -- |	
00 00 07 6E |	            ALUR ALUA //RW bits to ALU
00 00 07 70 |	            1E ALUB //shift 30 bits right
00 00 07 72 |	            06 ALUM //right shift mode
-- -- -- -- |	
-- -- -- -- |	        //compare PTE RW bits to !R,W (0x01)
00 00 07 74 |	            ALUR COMPA //RW bits to comparator
00 00 07 76 |	            01 COMPB //!R,W (0x01)
00 00 07 78 |	            COMPR PC
00 00 07 7A |	            :assertion_passed_faulted_pte_not_0rw1
00 00 07 7E |	            :assertion_failed_faulted_pte_not_0rw1
-- -- -- -- |	
-- -- -- -- |	        @assertion_failed_faulted_pte_not_0rw1
-- -- -- -- |	        //if these are not the protection bits, neither the PDE nor PTE faulted?
-- -- -- -- |	        //we are in the !R,W branch, so one of PDE and PTE should have !R,W
-- -- -- -- |	        //since neither meet that criteria, HALT; PANIC
00 00 07 82 |	            7F FLAG
-- -- -- -- |	
-- -- -- -- |	    @assertion_passed_faulted_pte_not_0rw1
-- -- -- -- |	    //update the PDE to be F,D
00 00 07 84 |	        ISTACK_fetched_pde ROFST
00 00 07 86 |	        RMEM GPC //GPC = PDE we need to update
-- -- -- -- |	        //GPB already holds PTE we need to update
-- -- -- -- |	
00 00 07 88 |	        GPC ALUA //PDE to ALU
00 00 07 8A |	        IADF ALUB //mask to ALU
00 00 07 8C |	        SKIP PC
00 00 07 8E |	        30 00 00 00 //F,D mask
00 00 07 92 |	        07 ALUM //OR mode
-- -- -- -- |	
-- -- -- -- |	    //write updated PDE back to page directory
00 00 07 94 |	        ISTACK_fetched_pde_kpa ROFST
00 00 07 96 |	        RMEM RBASE
00 00 07 98 |	        00 ROFST
00 00 07 9A |	        ALUR RMEM
-- -- -- -- |	
-- -- -- -- |	    //update the PTE to be R,!W,!F,!D
00 00 07 9C |	        GPB ALUA //PTE to ALU
00 00 07 9E |	        IADF ALUB //mask to ALU
00 00 07 A0 |	        SKIP PC
00 00 07 A2 |	        80 00 00 00 //R,!W,!F,!D mask
-- -- -- -- |	
00 00 07 A6 |	    BREAK
-- -- -- -- |	
-- -- -- -- |	    //update the PTE with the physical page where the page now lives
-- -- -- -- |	    //  we're replacing the disk block in the PTE, but we already saved it
-- -- -- -- |	    //  in the kernel's physical page map
-- -- -- -- |	
-- -- -- -- |	        // erase the disk block part of the PTE
00 00 07 A8 |	            ALUR ALUA //updated PTE to ALU
00 00 07 AA |	            IADF ALUB //mask to ALU
00 00 07 AC |	            SKIP PC
00 00 07 AE |	            FF F0 00 00 //disk block mask
00 00 07 B2 |	            08 ALUM //AND mode
-- -- -- -- |	
00 00 07 B4 |	            ALUR ALUA //PTE with disk block removed to ALU
00 00 07 B6 |	            WBASE RBASE
00 00 07 B8 |	            ISTACK_target_ppage ROFST
00 00 07 BA |	            RMEM ALUB //physical page where page now resides to ALU
00 00 07 BC |	            07 ALUM //OR mode
-- -- -- -- |	
-- -- -- -- |	        //write updated PTE back to page table
00 00 07 BE |	            WBASE RBASE
00 00 07 C0 |	            ISTACK_fetched_pte_kpa ROFST
00 00 07 C2 |	            RMEM RBASE
00 00 07 C4 |	            00 ROFST
00 00 07 C6 |	            ALUR RMEM
-- -- -- -- |	
-- -- -- -- |	@conclude_r0w1
-- -- -- -- |	//return from interrupt
-- -- -- -- |	//TODO implement this
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	//nothing left
00 00 07 C8 |	02 FPUM
00 00 07 CA |	11 FPUA
00 00 07 CC |	22 FPUB
00 00 07 CE |	FPUR GPC
00 00 07 D0 |	7F FLAG
-- -- -- -- |	// end interrupt handler
-- -- -- -- |	
-- -- -- -- |	FRAME_END
