-- -- -- -- |	//=========================================================================
-- -- -- -- |	// kernel entry point
-- -- -- -- |	//=========================================================================
-- -- -- -- |	
00 00 00 00 |	FILLB0
-- -- -- -- |	
-- -- -- -- |	//set the MMU VA break point
00 00 00 B0 |	IADF WBASE
00 00 00 B2 |	SKIP PC
00 00 00 B4 |	00 00 10 00 //MMIO base address (in VA)
00 00 00 B8 |	08 WOFST //breakpoint register
00 00 00 BA |	00 WMEM //breakpoint at 0
-- -- -- -- |	
-- -- -- -- |	//enable the breakpoint
00 00 00 BC |	14 WOFST //breakpoint enabled register
00 00 00 BE |	01 WMEM //non-zero -> enabled
-- -- -- -- |	
00 00 00 C0 |	00 RBASE
00 00 00 C2 |	00 ROFST
-- -- -- -- |	
-- -- -- -- |	//jump to 0 in user space
00 00 00 C4 |	00 PC
-- -- -- -- |	
-- -- -- -- |	//=========================================================================
-- -- -- -- |	// interrupt handler
-- -- -- -- |	//=========================================================================
00 00 00 C6 |	FILL200
00 00 02 00 |	WBASE DMEM00
00 00 02 02 |	WOFST DMEM04
-- -- -- -- |	
-- -- -- -- |	//TODO change this to save registers to kernel's stack
-- -- -- -- |	//dump registers to VA 0100
00 00 02 04 |	IADF WBASE
00 00 02 06 |	SKIP PC
00 00 02 08 |	00 00 01 00
00 00 02 0C |	00 WOFST
00 00 02 0E |	GPA WMEM
00 00 02 10 |	04 WOFST
00 00 02 12 |	GPB WMEM
00 00 02 14 |	08 WOFST
00 00 02 16 |	GPC WMEM
00 00 02 18 |	0C WOFST
00 00 02 1A |	GPD WMEM
00 00 02 1C |	10 WOFST
00 00 02 1E |	GPE WMEM
00 00 02 20 |	14 WOFST
00 00 02 22 |	GPF WMEM
00 00 02 24 |	18 WOFST
00 00 02 26 |	GPG WMEM
00 00 02 28 |	1C WOFST
00 00 02 2A |	GPH WMEM
-- -- -- -- |	
00 00 02 2C |	20 WOFST
00 00 02 2E |	COMPA WMEM
00 00 02 30 |	24 WOFST
00 00 02 32 |	COMPB WMEM
00 00 02 34 |	28 WOFST
00 00 02 36 |	RBASE WMEM
00 00 02 38 |	2C WOFST
00 00 02 3A |	ROFST WMEM
00 00 02 3C |	30 WOFST
00 00 02 3E |	ALUM WMEM
00 00 02 40 |	34 WOFST
00 00 02 42 |	ALUA WMEM
00 00 02 44 |	38 WOFST
00 00 02 46 |	ALUB WMEM
00 00 02 48 |	3C WOFST
00 00 02 4A |	FPUM WMEM
00 00 02 4C |	40 WOFST
00 00 02 4E |	FPUA WMEM
00 00 02 50 |	44 WOFST
00 00 02 52 |	FPUB WMEM
-- -- -- -- |	
-- -- -- -- |	//determine the cause of the interrupt
-- -- -- -- |	//mask flag to check if interrupt was raised on MMU channel
00 00 02 54 |	FLAG ALUA
00 00 02 56 |	IADF ALUB
00 00 02 58 |	SKIP PC
00 00 02 5A |	00 00 00 01 //mask for the first channel (MMU)
00 00 02 5E |	08 ALUM //AND mode
00 00 02 60 |	ALUR COMPA
00 00 02 62 |	00 COMPB //if flag channel is 1, jump to mmu interrupt handler
00 00 02 64 |	COMPR PC
00 00 02 66 |	:non_mmu_interrupt
00 00 02 6A |	:mmu_interrupt
-- -- -- -- |	
-- -- -- -- |	@non_mmu_interrupt
-- -- -- -- |	//if interrupt is not from MMU
-- -- -- -- |	//do nothing if the interrupt is from anywhere besides the MMU
00 00 02 6E |	7F FLAG
-- -- -- -- |	
-- -- -- -- |	@mmu_interrupt
-- -- -- -- |	//else interrupt is from MMU
-- -- -- -- |	//retrieve the faulted PTE from the MMU
00 00 02 70 |	    IADF RBASE
00 00 02 72 |	    SKIP PC
00 00 02 74 |	    00 00 10 00
00 00 02 78 |	    0C ROFST
00 00 02 7A |	    RMEM GPA
-- -- -- -- |	
-- -- -- -- |	//retrieve the faulted address from the MMU
00 00 02 7C |	    10 ROFST
00 00 02 7E |	    RMEM GPB
-- -- -- -- |	
-- -- -- -- |	//get the active process ID and page directory physical page from kernel memory
00 00 02 80 |	    00 ROFST
00 00 02 82 |	    IADF RBASE
00 00 02 84 |	    SKIP PC
00 00 02 86 |	    ::ACTIVE_PROCESS_ID
00 00 02 8A |	    RMEM GPC
00 00 02 8C |	    IADF RBASE
00 00 02 8E |	    SKIP PC
00 00 02 90 |	    ::ACTIVE_PROCESS_PAGE_DIRECTORY_PHYSICAL_PAGE
00 00 02 94 |	    RMEM GPD
-- -- -- -- |	
-- -- -- -- |	//check PTE protections
-- -- -- -- |	//get them from PTE
00 00 02 96 |	GPA ALUA //PTE to ALU
00 00 02 98 |	IADF ALUB //mask to ALU
00 00 02 9A |	SKIP PC
00 00 02 9C |	C0 00 00 00 //mask for the RW bits of the PTE
00 00 02 A0 |	08 ALUM //AND mode
-- -- -- -- |	
00 00 02 A2 |	ALUR ALUA //RW bits to ALU
00 00 02 A4 |	1E ALUB //shift 30 bits right
00 00 02 A6 |	06 ALUM //right shift mode
-- -- -- -- |	
-- -- -- -- |	//see which of 00, 01, 10, 11 the RW bits are
00 00 02 A8 |	ALUR COMPA
00 00 02 AA |	00 COMPB
00 00 02 AC |	COMPR PC
00 00 02 AE |	:r0w0
00 00 02 B2 |	:_r0w0
-- -- -- -- |	@_r0w0
00 00 02 B6 |	01 COMPB
00 00 02 B8 |	COMPR PC
00 00 02 BA |	:r0w1
00 00 02 BE |	:_r0w1
-- -- -- -- |	@_r0w1
00 00 02 C2 |	02 COMPB
00 00 02 C4 |	COMPR PC
00 00 02 C6 |	:r1w0
00 00 02 CA |	:_r1w0
-- -- -- -- |	@_r1w0
00 00 02 CE |	7F FLAG //halt; RW was 11, no page fault should have occurred
-- -- -- -- |	
-- -- -- -- |	@r0w0
-- -- -- -- |	//not mapped (possibly syscall)
00 00 02 D0 |	7F FLAG //TODO for now, just halt
-- -- -- -- |	
-- -- -- -- |	@r1w0
-- -- -- -- |	//page is readable but not writable
-- -- -- -- |	//either a shared page or a clean page
-- -- -- -- |	//page needs to be split or marked as dirty
00 00 02 D2 |	7F FLAG //TODO for now, just halt
-- -- -- -- |	
-- -- -- -- |	@r0w1
-- -- -- -- |	//not readable and "writable" indicates the page is mapped but paged out
-- -- -- -- |	//page not resident
-- -- -- -- |	
-- -- -- -- |	//set up the stack
00 00 02 D4 |	00 WOFST
00 00 02 D6 |	IADF WBASE
00 00 02 D8 |	SKIP PC
00 00 02 DA |	::KERNEL_STACK
-- -- -- -- |	
-- -- -- -- |	//push function address onto stack
00 00 02 DE |	IADF WMEM
00 00 02 E0 |	SKIP PC
00 00 02 E2 |	::get_open_physical_page
00 00 02 E6 |	04 WOFST
00 00 02 E8 |	RTRN LINK
00 00 02 EA |	IADN PC
00 00 02 EC |	::FUNC
-- -- -- -- |	
-- -- -- -- |	//result of function call is target physical page number
-- -- -- -- |	//pop result from stack
00 00 02 F0 |	00 WOFST
00 00 02 F2 |	WMEM GPH
-- -- -- -- |	
-- -- -- -- |	//load page from disk
-- -- -- -- |	//get disk block from PTE
00 00 02 F4 |	    GPA ALUA //PTE to ALU
00 00 02 F6 |	    IADF ALUB //mask to ALU
00 00 02 F8 |	    SKIP PC
00 00 02 FA |	    00 0F FF FF
00 00 02 FE |	    08 ALUM //AND mode
00 00 03 00 |	    ALUR GPE
-- -- -- -- |	//get virtual page / virtual directory number from virtual address
00 00 03 02 |	    GPB ALUA //virtual address to ALU
00 00 03 04 |	    IADF ALUB //mask to ALU
00 00 03 06 |	    SKIP PC
00 00 03 08 |	    FF FF F0 00 //remove physical offset part of virtual address
00 00 03 0C |	    ALUR GPF
-- -- -- -- |	
-- -- -- -- |	//load from disk
-- -- -- -- |	    //point RMEM to virtual disk
00 00 03 0E |	        IADF RBASE
00 00 03 10 |	        SKIP PC
00 00 03 12 |	        00 00 10 1C //MMIO starts at VA 1000
-- -- -- -- |	    //tell disk the target physical page (stored in GPH)
00 00 03 16 |	        00 ROFST
00 00 03 18 |	        GPH RMEM
-- -- -- -- |	    //tell disk target disk block (stored in GPE)
00 00 03 1A |	        04 ROFST
00 00 03 1C |	        GPE RMEM
-- -- -- -- |	    //use read mode
00 00 03 1E |	        08 ROFST
00 00 03 20 |	        00 RMEM
-- -- -- -- |	    //initiate transfer from disk to memory
00 00 03 22 |	        0C ROFST
00 00 03 24 |	        01 RMEM
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	//update physical page map
-- -- -- -- |	    //point RMEM to physical page map array
00 00 03 26 |	        IADF RBASE
00 00 03 28 |	        SKIP PC
00 00 03 2A |	        ::physical_page_map
-- -- -- -- |	
-- -- -- -- |	    //calculate offset of target physical page entry
00 00 03 2E |	        02 ALUM //multiply
00 00 03 30 |	        GPH ALUA //target physical page
00 00 03 32 |	        14 ALUB //20 bytes per entry
00 00 03 34 |	        ALUR ROFST //physical page map offset
-- -- -- -- |	
00 00 03 36 |	    GPC RMEM //set process ID to user process
-- -- -- -- |	
-- -- -- -- |	    //move to next field in entry
00 00 03 38 |	        01 ALUM //add
00 00 03 3A |	        ROFST ALUA
00 00 03 3C |	        04 ALUB
00 00 03 3E |	        ALUR ROFST
-- -- -- -- |	
00 00 03 40 |	    GPD RMEM //set directory physical page to user directory
-- -- -- -- |	
-- -- -- -- |	    //move to next field
00 00 03 42 |	        ALUR ALUA
00 00 03 44 |	        ALUR ROFST
-- -- -- -- |	        
00 00 03 46 |	    GPF RMEM //set virtual page/directory number to that of the faulted address
-- -- -- -- |	
-- -- -- -- |	    //move to next field
00 00 03 48 |	        ALUR ALUA
00 00 03 4A |	        ALUR ROFST
-- -- -- -- |	
00 00 03 4C |	    01 RMEM //only one process references this page
-- -- -- -- |	
-- -- -- -- |	    //move to next field
00 00 03 4E |	        ALUR ALUA
00 00 03 50 |	        ALUR ROFST
-- -- -- -- |	
00 00 03 52 |	    GPE RMEM //store origin disk block as the disk block
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	//update process map
-- -- -- -- |	    //point RMEM to process map
00 00 03 54 |	        IADF RBASE
00 00 03 56 |	        SKIP PC
00 00 03 58 |	        ::process_map
-- -- -- -- |	
00 00 03 5C |	    BREAK
-- -- -- -- |	
-- -- -- -- |	    //calculate offset of target process entry ((process ID - 1) is index into map)
-- -- -- -- |	        // (16 * (PID - 1)) + 8
00 00 03 5E |	        03 ALUM //subtract
00 00 03 60 |	        GPC ALUA //process ID
00 00 03 62 |	        01 ALUB //minus 1
00 00 03 64 |	        ALUR ALUA //ALUA = PID - 1
-- -- -- -- |	
00 00 03 66 |	        10 ALUB //16 byte per entry
00 00 03 68 |	        02 ALUM //multiply
00 00 03 6A |	        ALUR ALUA //process entry map offset
00 00 03 6C |	        08 ALUB //offset into entry for number of resident pages
00 00 03 6E |	        01 ALUM //add
00 00 03 70 |	        ALUR ROFST //total offset into process map
-- -- -- -- |	
-- -- -- -- |	    //increment number of resident pages for this process
00 00 03 72 |	        RMEM ALUA
00 00 03 74 |	        01 ALUB
00 00 03 76 |	        ALUR RMEM
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	//update page table
-- -- -- -- |	
-- -- -- -- |	//return from interrupt
-- -- -- -- |	
-- -- -- -- |	
-- -- -- -- |	//nothing left
00 00 03 78 |	02 FPUM
00 00 03 7A |	11 FPUA
00 00 03 7C |	22 FPUB
00 00 03 7E |	FPUR GPC
00 00 03 80 |	7F FLAG
-- -- -- -- |	
00 00 03 82 |	FILL600
